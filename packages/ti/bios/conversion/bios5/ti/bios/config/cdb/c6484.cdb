	},
	$a = ((float((CLK . PRD)) + CLK . adjustPrd) * ((CLK . TDDR) + 1.0) * 1 / CLK . _INPUTCLK),
	$b = CLK.MICROSECONDS,
	$e = "ok",
	if (($e = .setMicroseconds(CLK.MICROSECONDS)) == "ok") {
	    if (CLK.gGBLChain != nil) {
