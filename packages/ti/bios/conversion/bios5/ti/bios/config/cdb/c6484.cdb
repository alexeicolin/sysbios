		(1 * (.TDDR + 1.0))) - .adjustPrd,
	    .MICROSECONDS = ((float((. PRD)) + CLK . adjustPrd) * ((. TDDR) + 1.0) * 1 / CLK . _INPUTCLK),
	    if (.CALLBACKOBJ != nil) {
		.CALLBACKOBJ.call()
